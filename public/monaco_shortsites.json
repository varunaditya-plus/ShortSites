{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    { "token": "", "foreground": "fafafa", "background": "1a1a1a" },
    { "token": "invalid", "foreground": "ff5555" },
    { "token": "emphasis", "fontStyle": "italic" },
    { "token": "strong", "fontStyle": "bold" },
    { "token": "variable", "foreground": "8be9fd" },
    { "token": "variable.predefined", "foreground": "ffb86c" },
    { "token": "variable.parameter", "foreground": "8be9fd" },
    { "token": "constant", "foreground": "f1fa8c" },
    { "token": "comment", "foreground": "42424a" },
    { "token": "number", "foreground": "f1fa8c" },
    { "token": "number.hex", "foreground": "f1fa8c" },
    { "token": "regexp", "foreground": "ff79c6" },
    { "token": "annotation", "foreground": "ff5555" },
    { "token": "type", "foreground": "8be9fd" },
    { "token": "delimiter", "foreground": "fafafa" },
    { "token": "delimiter.html", "foreground": "fafafa" },
    { "token": "delimiter.xml", "foreground": "fafafa" },
    { "token": "tag", "foreground": "c084fc" },
    { "token": "tag.id.jade", "foreground": "ffb86c" },
    { "token": "tag.class.jade", "foreground": "ffb86c" },
    { "token": "meta.scss", "foreground": "ff79c6" },
    { "token": "meta.tag", "foreground": "c084fc" },
    { "token": "metatag", "foreground": "ff79c6" },
    { "token": "metatag.content.html", "foreground": "c084fc" },
    { "token": "metatag.html", "foreground": "ffffff" },
    { "token": "metatag.xml", "foreground": "c084fc" },
    { "token": "metatag.php", "fontStyle": "bold", "foreground": "c084fc" },
    { "token": "key", "foreground": "8be9fd" },
    { "token": "string.key.json", "foreground": "8be9fd" },
    { "token": "string.value.json", "foreground": "56d4fb" },
    { "token": "attribute.name", "foreground": "ffb86c" },
    { "token": "attribute.value", "foreground": "56d4fb" },
    { "token": "attribute.value.number.css", "foreground": "f1fa8c" },
    { "token": "attribute.value.unit.css", "foreground": "f1fa8c" },
    { "token": "attribute.value.hex.css", "foreground": "fafafa" },
    { "token": "string", "foreground": "56d4fb" },
    { "token": "string.sql", "foreground": "ff5555" },
    { "token": "keyword", "foreground": "c084fc" },
    { "token": "keyword.flow", "foreground": "c084fc" },
    { "token": "keyword.json", "foreground": "c084fc" },
    { "token": "keyword.flow.scss", "foreground": "c084fc" },
    { "token": "operator.scss", "foreground": "fafafa" },
    { "token": "operator.sql", "foreground": "ff5555" },
    { "token": "operator.swift", "foreground": "fafafa" },
    { "token": "predefined.sql", "foreground": "ff79c6" }
  ],
  "colors": {
    "editor.foreground": "#fafafa",
    "editor.background": "#1a1a1a",
    "editor.selectionBackground": "#454545",
    "editor.lineHighlightBackground": "#2d2d2d",
    "editorCursor.foreground": "#c084fc",
    "editorWhitespace.foreground": "#454545",
    "editorIndentGuide.activeBackground": "#c084fc",
    "editor.selectionHighlightBorder": "#454545",
    "editorGutter.background": "#1a1a1a"
  }
}
